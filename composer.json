{
    "name": "craffft/contao-css-selector",
    "description": "CSS selection of predefined classes.",
    "keywords": ["css", "selector", "list", "classes"],
    "type": "contao-module",
    "homepage": "http://craffft.de",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Daniel Kiesel",
            "email": "daniel@craffft.de",
            "homepage": "http://craffft.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Craffft/contao-css-selector/issues",
        "source": "https://github.com/Craffft/contao-css-selector"
    },
    "require": {
        "php": ">=5.3.2",
        "contao/core": "~3.4",
        "contao-community-alliance/composer-installer": "*"
    },
    "autoload": {
        "classmap": ["system/"]
    },
    "replace": {
        "contao-legacy/css-selector": "*"
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/css-selector": "system/modules/css-selector"
            }
        }
    }
